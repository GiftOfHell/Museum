          "amount1" : {
            "type" : "string"
          },
          "amount2" : {
            "type" : "string"
          },
          "amount3" : {
            "type" : "string"
          },
          "date1" : {
            "type" : "string"
          },
          "date2" : {
            "type" : "string"
          },
          "date3" : {
            "type" : "string"
          }
        }
      },
      "ErrorResponse" : {
        "required" : [ "message" ],
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        }
      }
    }
  }
}


