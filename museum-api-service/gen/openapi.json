{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Location service",
    "version" : "0.1.0"
  },
  "servers" : [ {
    "url" : "http://localhost:10010/api/v1"
  } ],
  "paths" : {
    "/location" : {
      "get" : {
        "description" : "Returns list of museum locations",
        "operationId" : "locations",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "Filtering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/LocationsResponse"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "location-controller"
    },
    "/swagger" : {
      "x-swagger-pipe" : "swagger_raw"
    }
  },
  "components" : {
    "schemas" : {
      "LocationsResponse" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/Location"
        }
      },
      "Location" : {
        "required" : [ "country1" ],
        "type" : "object",
        "properties" : {
          "country1" : {
            "type" : "string"
          },
          "city1" : {
            "type" : "string"
          },
          "street1" : {
            "type" : "string"
          },
          "address1" : {
            "type" : "string"
          },
          "image1" : {
            "type" : "string"
          },
          "country2" : {
            "type" : "string"
          },
          "city2" : {
            "type" : "string"
          },
          "street2" : {
            "type" : "string"
          },
          "address2" : {
            "type" : "string"
          },
          "image2" : {
            "type" : "string"
          },
          "country3" : {
            "type" : "string"
          },
          "city3" : {
            "type" : "string"
          },
          "street3" : {
            "type" : "string"
          },
          "address3" : {
            "type" : "string"
          },
          "image3" : {
            "type" : "string"
          }
        }
      },
      "ErrorResponse" : {
        "required" : [ "message" ],
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        }
      }
    }
  }
}