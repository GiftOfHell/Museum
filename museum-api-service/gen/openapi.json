      "get" : {
        "description" : "Returns info about ticket",
        "operationId" : "tickets",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "some info about ticket",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
